<html>

<head>
    <title><%=title%></title>
    <link rel="stylesheet" href="/css/styles.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>

<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <ul class="nav navbar-nav">
            <%for(let i=0; i<nav.length; i++) {%>
            <li class="nav-item active">
                <a class="nav-link" href="<%=nav[i].link%>"><%=nav[i].title%></a>
            </li>
            <%}%>
                </ul>         
            </nav>
            <h1><%=title%></h1>
            <%for(let i=0; i<blogs.length; i++) {%>
            <div class="row">
                <div class="col-md-10 blog-font">
                    <div class="blog-row">
                        <div class=label><%=head1%></div><%=blogs[i].userId%>
                    </div>
                    <div class="blog-row">
                        <div class=label><%=head2%></div><%=blogs[i].id%>
                    </div>
                    <div class="blog-row">
                        <div class=label><%=head3%></div><%=blogs[i].title%>"
                    </div>
                    <div class="blog-row">
                        <div class=label><%=head4%></div><%=blogs[i].body%>"
                        <a href="/blogs/<%=i%>" class="link">See this blog</a>
                    </div>
                </div>
            </div>

            <%}%>
          
    </body>
</html>